{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-trailing-spaces": "warn",
        "no-empty": "warn",
        "no-multiple-empty-lines": "warn"
    }
}